		{
			"name": "Login User",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Verifica se a requisição atual é a de \"Login User\"\r",
							"if (pm.request.url.path.join('/') === 'users/login') {\r",
							"    pm.test(\"Login Successful\", function () {\r",
							"        pm.response.to.have.status(200);\r",
							"    });\r",
							"\r",
							"    // Captura o token da resposta JSON\r",
							"    var jsonData = pm.response.json();\r",
							"    if (jsonData.token) {\r",
							"        pm.collectionVariables.set(\"token\", jsonData.token); // Salva o token na variável de ambiente\r",
							"        console.log(\"Token salvo com sucesso: \" + jsonData.token);\r",
							"    } else {\r",
							"        console.warn(\"Token não encontrado na resposta.\");\r",
							"    }\r",
							"}\r",
							""
						],
